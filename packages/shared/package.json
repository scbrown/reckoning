{
  "name": "@reckoning/shared",
  "version": "0.1.0",
  "private": true,
  "description": "Shared types and utilities for Reckoning",
  "type": "module",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./tts": {
      "types": "./dist/tts/index.d.ts",
      "import": "./dist/tts/index.js"
    },
    "./game": {
      "types": "./dist/game/index.d.ts",
      "import": "./dist/game/index.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "typescript": "^5.3.3"
  }
}
